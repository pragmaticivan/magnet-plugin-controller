{
  "name": "magnet-plugin-controller",
  "version": "0.0.4",
  "description": "Magnet plugin for controller route support",
  "engines": {
    "node": ">=8.9.0"
  },
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "babel -d lib/ src/",
    "format": "prettier-eslint --write \"{src/**/*.js,test/**/*.js,*.js}\" && npm run format-imports && npm run lint",
    "format-imports": "import-sort -o src",
    "lint": "eslint src/**/*.js",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register test/setup/node.js test/unit/**/*.js"
  },
  "contributors": [
    "Ivan Santos <ivan.santos@liferay.com>"
  ],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/wedeploy/magnet-plugin-controller.git"
  },
  "dependencies": {
    "metal": "^2.16.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.15.0",
    "eslint-config-liferay": "^2.0.18",
    "magnet": "latest",
    "mocha": "^3.2.0",
    "import-sort-cli": "^3.3.0",
    "import-sort-parser-babylon": "^3.2.0",
    "import-sort-style-eslint": "^3.0.0",
    "prettier-eslint-cli": "^4.4.0"
  }
}
